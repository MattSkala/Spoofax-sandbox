module minijava

imports
  
  common

context-free start-symbols
  
  Program

context-free syntax
  Program.S   = E
  E.Var = ID
  E.Neg = "-" E
  E.Add = E "+" E	{left}
  E.Sub = E "-" E	{left}
  E.Div = E "/" E	{left}
  E.Mul = E "*" E	{left}
  E.Pow = E "^" E	{left}
  E.Par = "(" E ")"
 
 context-free priorities

	{ 
		E.Pow
	} > {
		E.Neg
  } > { left:
    E.Mul
    E.Div
  } > { left:
    E.Add
    E.Sub
  }
    